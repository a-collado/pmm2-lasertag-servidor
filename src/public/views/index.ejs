<!doctype html>
<html lang="es">
    <head>
        <meta charset="utf-8">
        <title>Slayser Tag</title>
    </head>
    <body>
        <main> 
            <% if (setting_teams) { %>
                <h1>Elegir equipos:</h1>
                <ul id="devices_teams"></ul>
                <button type="button" onclick="">Start Game</button>
                <button type="button" onclick="goToDevices()">Go Back</button>
            <% } else { %>
                <h1>Dispositivos conectados:</h1>
                <ul id="devices_connecting"></ul>
                <button type="button" onclick="selectTeams()">Start Game</button>
            <% } %>
        </main>
        <script type="text/javascript" src="/js/funciones.js"></script>
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-eOJMYsd53ii+scO/bJGFsiCZc+5NDVN2yr8+0RDqr0Ql0h+rP48ckxlpbzKgwra6" crossorigin="anonymous">
    </body>
</html>

